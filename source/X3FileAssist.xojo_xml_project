 <Method>
  <ItemName>action_Open</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub action_Open()</SourceLine>
   <SourceLine>Dim dlg As new OpenDialog</SourceLine>
   <SourceLine>Dim modFile As FolderItem</SourceLine>
   <SourceLine>Dim allType As new FileType</SourceLine>
   <SourceLine>Dim x3mType As new FileType</SourceLine>
   <SourceLine>Dim objType As new FileType</SourceLine>
   <SourceLine>Dim qdrawType As new FileType</SourceLine>
   <SourceLine>Dim io As new X3Core.X3IO</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>allType.Name = "All model files"</SourceLine>
   <SourceLine>allType.Extensions = ".x3m;.obj;.3dmf"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>x3mType.Name = "X3 Model" ' create X3 Model type</SourceLine>
   <SourceLine>x3mType.Extensions = ".x3m"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>objType.Name = "Wavefront OBJ"</SourceLine>
   <SourceLine>objType.Extensions = ".obj"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>qdrawType.Name = "QuickDraw 3DMF"</SourceLine>
   <SourceLine>qdrawType.Extensions = ".3dmf"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dlg.Filter = allType + x3mType + objType + qdrawType</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>modFile = dlg.ShowModal() ' get file to load model from</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if modFile &lt;&gt; nil then ' did the user press cancel?</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>lblFilename.Text = "Loading, please wait..."</SourceLine>
   <SourceLine>lblFilename.TextColor = RGB(200, 0, 0)</SourceLine>
   <SourceLine>lblFilename.Refresh</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if Right(modFile.Name, 4) = ".x3m" then ' .x3m</SourceLine>
   <SourceLine>CurrentModel = io.LoadModel(modFile)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif Right(modFile.Name, 4) = ".obj" then</SourceLine>
   <SourceLine>CurrentModel = LoadModelFromOBJ(modFile) ' .obj</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif Right(modFile.Name, 5) = ".3dmf" then</SourceLine>
   <SourceLine>CurrentModel = LoadModelFrom3DMF(modFile) ' .x3mf</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>CurrentFilename = modFile.Name</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>lblFilename.Text = modFile.NativePath</SourceLine>
   <SourceLine>lblFilename.TextColor = RGB(0, 0, 0)</SourceLine>
   <SourceLine>displayModelInfo</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
