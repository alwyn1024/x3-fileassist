   <SourceLine>Sub displayModelInfo()</SourceLine>
   <SourceLine>lstModelInfo.DeleteAllRows</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>lstModelInfo.AddRow "Colors", Str(CurrentModel.ColorPal.Ubound + 1)</SourceLine>
   <SourceLine>lstModelInfo.AddRow "Materials", Str(CurrentModel.Material.Ubound + 1)</SourceLine>
   <SourceLine>lstModelInfo.AddRow "Normals", Str(CurrentModel.Normal.Ubound + 1)</SourceLine>
   <SourceLine>lstModelInfo.AddRow "Polygons", Str(CurrentModel.Polygon.Ubound + 1)</SourceLine>
   <SourceLine>lstModelInfo.AddRow "Textures", Str(CurrentModel.Texture.Ubound + 1)</SourceLine>
   <SourceLine>lstModelInfo.AddRow "UV Coordinates", Str(CurrentModel.UVMap.Ubound + 1)</SourceLine>
   <SourceLine>lstModelInfo.AddRow "Vertices", Str(CurrentModel.Vertex.Ubound + 1)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
