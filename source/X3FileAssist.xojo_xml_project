   <SourceLine>Dim dataStr As String</SourceLine>
   <SourceLine>Dim dataPos As Integer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = false</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.NextToken() = "PixmapTexture" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match open bracket</SourceLine>
   <SourceLine>if tokenizer.NextToken() = "(" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>width = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>height = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? todo: what is value for?</SourceLine>
   <SourceLine>bytesPerToken = Val(tokenizer.NextToken()) ' ? todo: verify if correct?</SourceLine>
   <SourceLine>rgbType = tokenizer.NextToken()</SourceLine>
   <SourceLine>endian1 = tokenizer.NextToken()</SourceLine>
   <SourceLine>endian2 = tokenizer.NextToken()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>select case rgbType</SourceLine>
   <SourceLine>case "RGB16"</SourceLine>
   <SourceLine>pixelSize = 16</SourceLine>
   <SourceLine>case else</SourceLine>
   <SourceLine>pixelSize = 16</SourceLine>
   <SourceLine>end select</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>picData = new MemoryBlock(width * height * (pixelSize \ 8))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' parse picture data</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dataStr = tokenizer.NextToken()</SourceLine>
   <SourceLine>dataPos = 0</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>while dataStr &lt;&gt; ")"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' TODO: parse data</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dataStr = tokenizer.NextToken()</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if dataStr = ")" then</SourceLine>
   <SourceLine>result = true</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>break</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return result</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
