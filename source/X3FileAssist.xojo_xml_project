  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function LoadModelFrom3DMF(modelFile As FolderItem) As X3Core.X3Model</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee} initiative.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim model As new X3Core.X3Model</SourceLine>
   <SourceLine>Dim fileData As String</SourceLine>
   <SourceLine>Dim textInput As TextInputStream</SourceLine>
   <SourceLine>Dim tokenizer As QuickDrawTokenizer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' intialize variables</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>qdDiffuseIndex = 0</SourceLine>
   <SourceLine>qdNumEdgeAttrTypes = 0</SourceLine>
   <SourceLine>qdNumEdges = 0</SourceLine>
   <SourceLine>qdNumPoints = 0</SourceLine>
   <SourceLine>qdNumTriangleAttrTypes = 0</SourceLine>
   <SourceLine>qdNumTriangles = 0</SourceLine>
   <SourceLine>qdNumVertexAttrTypes = 0</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' read 3dmf data into memory block</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>textInput = TextInputStream.Open(modelFile)</SourceLine>
   <SourceLine>fileData = textInput.ReadAll()</SourceLine>
   <SourceLine>textInput.Close()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' instantiate tokenizer that will be used to parse 3DMF tokens</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tokenizer = new QuickDrawTokenizer(fileData)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>while qdMatch_Object(tokenizer, model)</SourceLine>
   <SourceLine>// keep om matching 3DMF objects until we get an errror</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return model</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
