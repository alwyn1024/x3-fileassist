 <Method>
  <ItemName>NextToken</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function NextToken() As String</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee} initiative.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim token() As String</SourceLine>
   <SourceLine>Dim foundToken As Boolean</SourceLine>
   <SourceLine>Dim tmpStr As String</SourceLine>
   <SourceLine>Dim ch As String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if not EOF then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// skip past whitespace</SourceLine>
   <SourceLine>while (Position &lt; mData.Size) and (mData.Byte(Position) &lt;= 32)</SourceLine>
   <SourceLine>Position = Position + 1</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' loop through data characters until we find whitespace or the end-of-file</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>foundToken = false</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>while not EOF and not foundToken</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>ch = Chr(mData.Byte(Position))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if (mData.Byte(Position) &lt;= 32)  then</SourceLine>
   <SourceLine>foundToken = true</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if ch = "#" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' skip all charecters up to end of line</SourceLine>
   <SourceLine>Position = Position + 1</SourceLine>
   <SourceLine>while (mData.Byte(Position) &lt;&gt; 10) and (mData.Byte(Position) &lt;&gt; 13)</SourceLine>
   <SourceLine>Position = Position + 1</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// skip past whitespace</SourceLine>
   <SourceLine>while mData.Byte(Position) &lt;= 32</SourceLine>
   <SourceLine>Position = Position + 1</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if token.Ubound &gt;= 0 then</SourceLine>
   <SourceLine>foundToken = true</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>select case ch</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "(", ")"</SourceLine>
   <SourceLine>foundToken = true</SourceLine>
   <SourceLine>if token.Ubound &lt; 0 then</SourceLine>
   <SourceLine>token.Append ch</SourceLine>
   <SourceLine>Position = Position + 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end select</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if not foundToken then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>token.Append ch</SourceLine>
   <SourceLine>Position = Position + 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return Join(token, "")</SourceLine>
   <SourceLine>End Function</SourceLine>
