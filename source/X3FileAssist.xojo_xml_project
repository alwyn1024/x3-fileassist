 <Method>
  <ItemName>action_Save</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub action_Save()</SourceLine>
   <SourceLine>Dim dlg As new SaveAsDialog</SourceLine>
   <SourceLine>Dim modFile As FolderItem</SourceLine>
   <SourceLine>Dim x3mType As new FileType</SourceLine>
   <SourceLine>Dim objType As new FileType</SourceLine>
   <SourceLine>Dim n As Integer</SourceLine>
   <SourceLine>Dim io as new X3Core.X3IO</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if CurrentModel &lt;&gt; nil then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>x3mType.Name = "X3 Model" ' create X3 Model type</SourceLine>
   <SourceLine>x3mType.Extensions = ".x3m"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>objType.Name = "Wavefront OBJ"</SourceLine>
   <SourceLine>objType.Extensions = ".obj"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dlg.Filter = x3mType + objType ' assign type to dialog</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if Right(CurrentFilename, 5) = ".3dmf" then</SourceLine>
   <SourceLine>dlg.SuggestedFileName = Left(CurrentFilename, Len(CurrentFilename) - 5)</SourceLine>
   <SourceLine>elseif Len(CurrentFilename) &gt; 4 then</SourceLine>
   <SourceLine>dlg.SuggestedFileName = Left(CurrentFilename, Len(CurrentFilename) - 4)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>modFile = dlg.ShowModal() ' get file to save model to</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if modFile &lt;&gt; nil then ' did the user press cancel?</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' make sure the file extension is correct before saving</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if Right(modFile.Name, 4) = ".x3m" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>lblFilename.Text = "Saving, please wait..."</SourceLine>
   <SourceLine>lblFilename.TextColor = RGB(200, 0, 0)</SourceLine>
   <SourceLine>lblFilename.Refresh</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>io.SaveModel(modFile, CurrentModel) ' save the model to the selected flie</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>CurrentFilename = modFile.Name</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>lblFilename.Text = modFile.NativePath</SourceLine>
   <SourceLine>lblFilename.TextColor = RGB(0, 0, 0)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif Right(modFile.Name, 4) = ".obj" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>lblFilename.Text = "Saving, please wait..."</SourceLine>
   <SourceLine>lblFilename.TextColor = RGB(200, 0, 0)</SourceLine>
   <SourceLine>lblFilename.Refresh</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>SaveModelToOBJ(CurrentModel, modFile)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>CurrentFilename = modFile.Name</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>lblFilename.Text = modFile.NativePath</SourceLine>
   <SourceLine>lblFilename.TextColor = RGB(0, 0, 0)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>n = MsgBox("[" + modFile.Name + "]" + EndOfLine + "Unknown file extension. Unable to save model.", 16, "Save Model")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
