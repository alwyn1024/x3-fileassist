 <Method>
  <ItemName>qdMatch_AttributeArray</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function qdMatch_AttributeArray(tokenizer As QuickDrawTokenizer, ByRef model As X3Core.X3Model) As Boolean</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee} initiative.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim result As Boolean</SourceLine>
   <SourceLine>Dim tmpStr As String</SourceLine>
   <SourceLine>Dim i As Integer</SourceLine>
   <SourceLine>Dim j As Integer</SourceLine>
   <SourceLine>Dim poly As X3Core.X3Polygon</SourceLine>
   <SourceLine>Dim tmpX As Double</SourceLine>
   <SourceLine>Dim tmpY As Double</SourceLine>
   <SourceLine>Dim tmpZ As Double</SourceLine>
   <SourceLine>Dim tmpVec As X3Core.X3Vector</SourceLine>
   <SourceLine>Dim tmpUV As X3Core.X3UVCoordinate</SourceLine>
   <SourceLine>Dim firstIndex As Integer</SourceLine>
   <SourceLine>Dim tmpInt As Integer</SourceLine>
   <SourceLine>Dim arrType As String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = false</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.NextToken() = "AttributeArray" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match open bracket</SourceLine>
   <SourceLine>if tokenizer.NextToken() = "(" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if qdNumTriangleAttrTypes &gt; 0 then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>arrType = tokenizer.LookToken()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' ===== triangle attribute ========</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if arrType = "3" then ' triangle normals</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>arrType = tokenizer.NextToken()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// ***** triangle normals ************</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for i = 1 to qdNumTriangles</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpX = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>tmpY = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>tmpZ = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpVec = new X3Core.X3Vector(tmpX, tmpY, tmpZ)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Normal.Append tmpVec</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Polygon(model.Polygon.Ubound - (qdNumTriangles - i)).NIndex = model.Normal.Ubound</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next i </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>break // ? unknown triangle attribute type</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>qdNumTriangleAttrTypes = qdNumTriangleAttrTypes - 1</SourceLine>
   <SourceLine>if qdNumTriangleAttrTypes &lt;= 0 then</SourceLine>
   <SourceLine>qdNumTriangles = 0</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif qdNumEdgeAttrTypes &gt; 0 then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' TODO </SourceLine>
   <SourceLine>break</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif qdNumVertexAttrTypes &gt; 0 then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>arrType = tokenizer.LookToken()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' ===== vertex attribute ========</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if arrType = "3" then ' vertex normals</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>arrType = tokenizer.NextToken()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// ***** vertex normals ************</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>firstIndex = model.Normal.Ubound + 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for i = 1 to qdNumPoints</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpX = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>tmpY = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>tmpZ = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpVec = new X3Core.X3Vector(tmpX, tmpY, tmpZ)</SourceLine>
   <SourceLine>model.Normal.Append tmpVec</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' update vertex normal indices</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpInt = model.Vertex.Ubound - qdNumPoints + 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for i = 0 to model.Polygon.Ubound</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly = model.Polygon(i)</SourceLine>
   <SourceLine>for j = 0 to poly.VIndex.Ubound</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if poly.VIndex(j) &gt;= tmpInt then</SourceLine>
   <SourceLine>poly.VNIndex.Append firstIndex + poly.VIndex(j) - tmpInt</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next j </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next i </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif arrType = "2" then ' u,v coordinates</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>arrType = tokenizer.NextToken()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ? unknown value</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>firstIndex = model.UVMap.Ubound + 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for i = 1 to qdNumPoints</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpX = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>tmpY = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpUV = new X3Core.X3UVCoordinate(tmpX, tmpY)</SourceLine>
   <SourceLine>model.UVMap.Append tmpUV</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' update vertex uv coordinates</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpInt = model.Vertex.Ubound - qdNumPoints + 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for i = 0 to model.Polygon.Ubound</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly = model.Polygon(i)</SourceLine>
   <SourceLine>for j = 0 to poly.VIndex.Ubound</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if poly.VIndex(j) &gt;= tmpInt then</SourceLine>
   <SourceLine>poly.UVIndex.Append firstIndex + poly.VIndex(j) - tmpInt</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next j</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>break // ? unknown triangle attribute type</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>qdNumVertexAttrTypes = qdNumVertexAttrTypes - 1</SourceLine>
   <SourceLine>if qdNumVertexAttrTypes &lt;= 0 then</SourceLine>
   <SourceLine>qdNumPoints = 0</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>break ' unexpected break point</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match closing bracket</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.NextToken() = ")" then</SourceLine>
   <SourceLine>result = true</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return result</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
