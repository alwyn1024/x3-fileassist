   <SourceLine></SourceLine>
   <SourceLine>modelJSON.Append "]" ' vertex</SourceLine>
   <SourceLine>addedChild = true</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
