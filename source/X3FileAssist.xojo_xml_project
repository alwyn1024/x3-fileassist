 <Method>
  <ItemName>LoadModelFromOBJ</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function LoadModelFromOBJ(modelFile As FolderItem) As X3Core.X3Model</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee} initiative.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim model As new X3Core.X3Model</SourceLine>
   <SourceLine>Dim tis As TextInputStream</SourceLine>
   <SourceLine>Dim sLine As String</SourceLine>
   <SourceLine>Dim tmpArr() As String</SourceLine>
   <SourceLine>Dim tmpArr2() As String</SourceLine>
   <SourceLine>Dim v As X3Core.X3Vector</SourceLine>
   <SourceLine>Dim uv As X3Core.X3UVCoordinate</SourceLine>
   <SourceLine>Dim f As X3Core.X3Polygon</SourceLine>
   <SourceLine>Dim i As Integer</SourceLine>
   <SourceLine>Dim matLib As new Dictionary</SourceLine>
   <SourceLine>Dim currentMat As String</SourceLine>
   <SourceLine>Dim textureIndex As Integer </SourceLine>
   <SourceLine>Dim colorIndex As Integer </SourceLine>
   <SourceLine>Dim materialIndex As Integer </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>colorIndex = -1</SourceLine>
   <SourceLine>textureIndex = -1</SourceLine>
   <SourceLine>materialIndex = - 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tis = TextInputStream.Open(modelFile)</SourceLine>
   <SourceLine>while not tis.EOF</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>sLine = Replace(Trim(tis.ReadLine()), Chr(9), " ")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if Left(sLine, 6) = "mtllib" then ' material library </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpArr = Split(sLine)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' remove empty values</SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>while i &lt;= tmpArr.Ubound</SourceLine>
   <SourceLine>if Trim(tmpArr(i)) = "" then</SourceLine>
   <SourceLine>tmpArr.Remove(i)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if UBound(tmpArr) &gt;= 1 then</SourceLine>
   <SourceLine>objLoadMaterials modelFile.Parent.Child(tmpArr(1)), matLib, model</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif Left(sLine, 1) = "f" then ' face (polygon)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>f = new X3Core.X3Polygon</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpArr = Split(sLine)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' remove empty values</SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>while i &lt;= tmpArr.Ubound</SourceLine>
   <SourceLine>if Trim(tmpArr(i)) = "" then</SourceLine>
   <SourceLine>tmpArr.Remove(i)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tmpArr.Ubound &gt;= 3 then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if textureIndex &gt;= 0  then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>f.TIndex = textureIndex</SourceLine>
   <SourceLine>for i = 1 to tmpArr.Ubound</SourceLine>
   <SourceLine>tmpArr2 = Split(tmpArr(i), "/")</SourceLine>
   <SourceLine>f.VIndex.Append Val(tmpArr2(0)) - 1</SourceLine>
   <SourceLine>f.UVIndex.Append Val(tmpArr2(1)) - 1</SourceLine>
   <SourceLine>if tmpArr2.Ubound &gt;= 2 then</SourceLine>
   <SourceLine>f.VNIndex.Append Val(tmpArr2(2)) - 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif materialIndex &gt;= 0 then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>f.MIndex = materialIndex</SourceLine>
   <SourceLine>for i = 1 to tmpArr.Ubound</SourceLine>
   <SourceLine>tmpArr2 = Split(tmpArr(i), "/")</SourceLine>
   <SourceLine>f.VIndex.Append Val(tmpArr2(0)) - 1</SourceLine>
   <SourceLine>if tmpArr2.Ubound &gt;= 2 then</SourceLine>
   <SourceLine>f.VNIndex.Append Val(tmpArr2(2)) - 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif colorIndex &gt;= 0 then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>f.CIndex = colorIndex</SourceLine>
   <SourceLine>for i = 1 to tmpArr.Ubound</SourceLine>
   <SourceLine>tmpArr2 = Split(tmpArr(i), "/")</SourceLine>
   <SourceLine>f.VIndex.Append Val(tmpArr2(0)) - 1</SourceLine>
   <SourceLine>if tmpArr2.Ubound &gt;= 2 then</SourceLine>
   <SourceLine>f.VNIndex.Append Val(tmpArr2(2)) - 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for i = 1 to tmpArr.Ubound</SourceLine>
   <SourceLine>tmpArr2 = Split(tmpArr(i), "/")</SourceLine>
   <SourceLine>f.VIndex.Append Val(tmpArr2(0)) - 1</SourceLine>
   <SourceLine>if tmpArr2.Ubound &gt;= 2 then</SourceLine>
   <SourceLine>f.VNIndex.Append Val(tmpArr2(2)) - 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.AppendPolygon f</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>'f.CalculateNormal()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif Left(sLine, 2) = "vn" then ' normal vector</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpArr = Split(sLine)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' remove empty values</SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>while i &lt;= tmpArr.Ubound</SourceLine>
   <SourceLine>if Trim(tmpArr(i)) = "" then</SourceLine>
   <SourceLine>tmpArr.Remove(i)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>v = new X3Core.X3Vector(Val(tmpArr(1)), Val(tmpArr(2)), Val(tmpArr(3)))</SourceLine>
   <SourceLine>v.Normalize()</SourceLine>
   <SourceLine>model.Normal.Append v</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif Left(sLine, 2) = "vt" then ' uv (texture) coordinate</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpArr = Split(sLine)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' remove empty values</SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>while i &lt;= tmpArr.Ubound</SourceLine>
   <SourceLine>if Trim(tmpArr(i)) = "" then</SourceLine>
   <SourceLine>tmpArr.Remove(i)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>uv = new X3Core.X3UVCoordinate(Val(tmpArr(1)), 1 - Val(tmpArr(2)))</SourceLine>
   <SourceLine>model.UVMap.Append uv</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif Left(sLine, 1) = "v" then ' vertex</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpArr = Split(sLine)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' remove empty values</SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>while i &lt;= tmpArr.Ubound</SourceLine>
   <SourceLine>if Trim(tmpArr(i)) = "" then</SourceLine>
   <SourceLine>tmpArr.Remove(i)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>v = new X3Core.X3Vector(Val(tmpArr(1)), Val(tmpArr(2)), Val(tmpArr(3)))</SourceLine>
   <SourceLine>model.Vertex.Append v</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elseif Left(sLine, 6) = "usemtl" then ' material library</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpArr = Split(sLine)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' remove empty values</SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>while i &lt;= tmpArr.Ubound</SourceLine>
   <SourceLine>if Trim(tmpArr(i)) = "" then</SourceLine>
   <SourceLine>tmpArr.Remove(i)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' clear texture settings</SourceLine>
   <SourceLine>textureIndex = -1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>currentMat = matLib.Lookup(tmpArr(1), "")</SourceLine>
   <SourceLine>if left(currentMat, 3) = "col" then ' color?</SourceLine>
   <SourceLine>colorIndex = Val(Right(currentMat, Len(currentMat) - 3))</SourceLine>
   <SourceLine>textureIndex = -1</SourceLine>
   <SourceLine>materialIndex = -1</SourceLine>
   <SourceLine>elseif left(currentMat, 3) = "mat" then ' material?</SourceLine>
   <SourceLine>materialIndex = Val(Right(currentMat, Len(currentMat) - 3))</SourceLine>
   <SourceLine>textureIndex = -1</SourceLine>
   <SourceLine>colorIndex = -1</SourceLine>
   <SourceLine>elseif left(currentMat, 3) = "txt" then ' texture?</SourceLine>
   <SourceLine>textureIndex = Val(Right(currentMat, Len(currentMat) - 3))</SourceLine>
   <SourceLine>colorIndex = -1</SourceLine>
   <SourceLine>materialIndex = -1</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>colorIndex = -1</SourceLine>
   <SourceLine>textureIndex = -1</SourceLine>
   <SourceLine>materialIndex = -1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return model</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
