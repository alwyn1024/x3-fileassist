 <Method>
  <ItemName>objLoadMaterials</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub objLoadMaterials(matFile As FolderItem, matLib As Dictionary, model As X3Core.X3Model)</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee} initiative.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim tis As TextInputStream</SourceLine>
   <SourceLine>Dim sLine As String</SourceLine>
   <SourceLine>Dim parseMat As Boolean </SourceLine>
   <SourceLine>Dim i As Integer</SourceLine>
   <SourceLine>Dim tmpArr() As String</SourceLine>
   <SourceLine>Dim t As X3Core.X3Texture</SourceLine>
   <SourceLine>Dim Ka As X3Core.X3Color</SourceLine>
   <SourceLine>Dim Kd As X3Core.X3Color</SourceLine>
   <SourceLine>Dim Ks As X3Core.X3Color</SourceLine>
   <SourceLine>Dim Ns As Double</SourceLine>
   <SourceLine>Dim d As Double</SourceLine>
   <SourceLine>Dim mat As X3Core.X3Material</SourceLine>
   <SourceLine>Dim matName As String</SourceLine>
   <SourceLine>Dim map_Kd As String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if matFile.Exists then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>parseMat = false</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tis = TextInputStream.Open(matFile)</SourceLine>
   <SourceLine>while not tis.EOF</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>sLine = Replace(Trim(tis.ReadLine()), Chr(9), " ")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if Left(sLine, 6) = "newmtl" then ' material library </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if parseMat then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' ----- add material ---------------------</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if map_Kd &lt;&gt; "" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if matFile.Parent.Child(map_Kd).Exists then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>t = loadTexture(matFile.Parent.Child(map_Kd))</SourceLine>
   <SourceLine>model.Texture.Append t</SourceLine>
   <SourceLine>t = nil</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>matLib.Value(matName) = "txt" + Str(model.Texture.Ubound)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>mat = new X3Core.X3Material()</SourceLine>
   <SourceLine>if Ka &lt;&gt; nil then</SourceLine>
   <SourceLine>mat.Ambient = Ka</SourceLine>
   <SourceLine>mat.Ambient.Alpha = d</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>if Kd &lt;&gt; nil then</SourceLine>
   <SourceLine>mat.Diffuse = Kd</SourceLine>
   <SourceLine>mat.Diffuse.Alpha = d</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>if Ks &lt;&gt; nil then</SourceLine>
   <SourceLine>mat.Specular = Ks</SourceLine>
   <SourceLine>mat.Specular.Alpha = d</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>mat.Shininess = Ns</SourceLine>
   <SourceLine>model.Material.Append mat</SourceLine>
   <SourceLine>matLib.Value(matName) = "mat" + Str(model.Material.Ubound)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' ----------------------------------------</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpArr = Split(sLine)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' remove empty values</SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>while i &lt;= tmpArr.Ubound</SourceLine>
   <SourceLine>if Trim(tmpArr(i)) = "" then</SourceLine>
   <SourceLine>tmpArr.Remove(i)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' set name, and clear texture properties</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>matName = tmpArr(1)</SourceLine>
   <SourceLine>map_Kd = ""</SourceLine>
   <SourceLine>Ka = nil</SourceLine>
   <SourceLine>Kd = nil</SourceLine>
   <SourceLine>Ks = nil</SourceLine>
   <SourceLine>Ns = 0</SourceLine>
   <SourceLine>d = 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>parseMat = true</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if parseMat then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpArr = Split(sLine)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' remove empty values</SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>while i &lt;= tmpArr.Ubound</SourceLine>
   <SourceLine>if Trim(tmpArr(i)) = "" then</SourceLine>
   <SourceLine>tmpArr.Remove(i)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tmpArr.Ubound &gt;= 0 then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>select case tmpArr(0)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "d"</SourceLine>
   <SourceLine>d = Val(tmpArr(1))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "Ka"</SourceLine>
   <SourceLine>Ka = new X3Core.X3Color(Val(tmpArr(1)), Val(tmpArr(2)), Val(tmpArr(3)))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "Kd"</SourceLine>
   <SourceLine>Kd = new X3Core.X3Color(Val(tmpArr(1)), Val(tmpArr(2)), Val(tmpArr(3)))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "Ks"</SourceLine>
   <SourceLine>Ks = new X3Core.X3Color(Val(tmpArr(1)), Val(tmpArr(2)), Val(tmpArr(3)))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "map_Kd"</SourceLine>
   <SourceLine>if tmpArr.Ubound &gt;= 1 then</SourceLine>
   <SourceLine>map_Kd = tmpArr(1)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "Ns"</SourceLine>
   <SourceLine>Ns = (Val(tmpArr(1)) / 1000) * 128</SourceLine>
   <SourceLine>//Ns = Val(tmpArr(1)) </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end select</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if parseMat then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' ----- add material ---------------------</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if map_Kd &lt;&gt; "" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if matFile.Parent.Child(map_Kd).Exists then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>t = loadTexture(matFile.Parent.Child(map_Kd))</SourceLine>
   <SourceLine>model.Texture.Append t</SourceLine>
   <SourceLine>t = nil</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>matLib.Value(matName) = "txt" + Str(model.Texture.Ubound)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>mat = new X3Core.X3Material()</SourceLine>
   <SourceLine>if Ka &lt;&gt; nil then</SourceLine>
   <SourceLine>mat.Ambient = Ka</SourceLine>
   <SourceLine>mat.Ambient.Alpha = d</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>if Kd &lt;&gt; nil then</SourceLine>
   <SourceLine>mat.Diffuse = Kd</SourceLine>
   <SourceLine>mat.Diffuse.Alpha = d</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>if Ks &lt;&gt; nil then</SourceLine>
   <SourceLine>mat.Specular = Ks</SourceLine>
   <SourceLine>mat.Specular.Alpha = d</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>mat.Shininess = Ns</SourceLine>
   <SourceLine>model.Material.Append mat</SourceLine>
   <SourceLine>matLib.Value(matName) = "mat" + Str(model.Material.Ubound)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' ----------------------------------------</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
