  <ItemName>qdMatch_TriMesh</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function qdMatch_TriMesh(tokenizer As QuickDrawTokenizer, ByRef model As X3Core.X3Model) As Boolean</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee} initiative.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim result As Boolean</SourceLine>
   <SourceLine>Dim tmpStr As String</SourceLine>
   <SourceLine>Dim vec as X3Core.X3Vector</SourceLine>
   <SourceLine>Dim poly As X3Core.X3Polygon</SourceLine>
   <SourceLine>Dim i As Integer</SourceLine>
   <SourceLine>Dim tmpX As Double</SourceLine>
   <SourceLine>Dim tmpY As Double</SourceLine>
   <SourceLine>Dim tmpZ As Double</SourceLine>
   <SourceLine>Dim vertexOffset As Integer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = false</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.NextToken() = "TriMesh" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match open bracket</SourceLine>
   <SourceLine>if tokenizer.NextToken() = "(" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' get data counters</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>qdNumTriangles = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>qdNumTriangleAttrTypes = qdNumTriangleAttrTypes + Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>qdNumEdges = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>qdNumEdgeAttrTypes = qdNumEdgeAttrTypes + Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>qdNumPoints = Val(tokenizer.NextToken()) </SourceLine>
   <SourceLine>qdNumVertexAttrTypes = qdNumVertexAttrTypes + Val(tokenizer.NextToken())</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' parse triangles</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>vertexOffset = model.Vertex.Ubound + 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for i  = 1 to qdNumTriangles</SourceLine>
   <SourceLine>poly  = new X3Core.X3Polygon()</SourceLine>
   <SourceLine>poly.VIndex.Append vertexOffset + Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>poly.VIndex.Append vertexOffset + Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>poly.VIndex.Append vertexOffset + Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>next i </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' parse points</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for i = 1 to qdNumPoints</SourceLine>
   <SourceLine>tmpX = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>tmpY = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>tmpZ = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>vec = new X3Core.X3Vector(tmpX, tmpY, tmpZ)</SourceLine>
   <SourceLine>model.Vertex.Append vec</SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' parse bounding box</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ?</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ?</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ?</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ?</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ?</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ?</SourceLine>
   <SourceLine>tmpStr = tokenizer.NextToken() ' ?</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match closing bracket</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.NextToken() = ")" then</SourceLine>
   <SourceLine>result = true</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
