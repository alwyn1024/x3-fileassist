 <Method>
  <ItemName>qdMatch_Object</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function qdMatch_Object(tokenizer As QuickDrawTokenizer, ByRef model As X3Core.X3Model) As Boolean</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee} initiative.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim match As Boolean</SourceLine>
   <SourceLine>Dim token As String</SourceLine>
   <SourceLine>Dim objectName As String</SourceLine>
   <SourceLine>Dim currentPosition As Integer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>match = false ' by default we assume it is not a match</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>currentPosition = tokenizer.Position ' store current position</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>objectName = tokenizer.LookToken()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if Right(objectName, 1) = ":" then ' is this a label</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>objectName = tokenizer.NextToken()</SourceLine>
   <SourceLine>match = true ' yes, so indicate that we found a match</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>select case objectName</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "", ")"</SourceLine>
   <SourceLine>match = false</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "AttributeArray"</SourceLine>
   <SourceLine>match = qdMatch_AttributeArray(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "AttributeSet"</SourceLine>
   <SourceLine>match = qdMatch_AttributeSet(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "3DMetafile"</SourceLine>
   <SourceLine>match = qdMatch_3DMetaFile(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "Box"</SourceLine>
   <SourceLine>match = qdMatch_Box(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "Container"</SourceLine>
   <SourceLine>match = qdMatch_Container(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "DiffuseColor"</SourceLine>
   <SourceLine>match = qdMatch_DiffuseColor(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "FaceAttributeSetList"</SourceLine>
   <SourceLine>match = qdMatch_FaceAttributeSetList(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "PixmapTexture"</SourceLine>
   <SourceLine>match = qdMatch_PixmapTexture(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "TextureShader"</SourceLine>
   <SourceLine>match = qdMatch_TextureShader(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case "TriMesh"</SourceLine>
   <SourceLine>match = qdMatch_TriMesh(tokenizer, model)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case else</SourceLine>
   <SourceLine>break</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end select</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if not match then</SourceLine>
   <SourceLine>tokenizer.Position = currentPosition ' no match, so restore old position</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return match</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>tokenizer As QuickDrawTokenizer, ByRef model As X3Core.X3Model</ItemParams>
  <ItemResult>Boolean</ItemResult>
 </Method>
 <Method>
  <ItemName>qdMatch_Container</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function qdMatch_Container(tokenizer As QuickDrawTokenizer, ByRef model As X3Core.X3Model) As Boolean</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee} initiative.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim result As Boolean</SourceLine>
   <SourceLine>Dim temporaryString As String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = false</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.NextToken() = "Container" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match open bracket</SourceLine>
   <SourceLine>if tokenizer.NextToken() = "(" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match container objects</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>while qdMatch_Object(tokenizer, model)</SourceLine>
   <SourceLine>// loop until we don't get a match for an object</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match closing bracket</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.NextToken() = ")" then</SourceLine>
   <SourceLine>result = true</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return result</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>tokenizer As QuickDrawTokenizer, ByRef model As X3Core.X3Model</ItemParams>
  <ItemResult>Boolean</ItemResult>
 </Method>
 <Method>
  <ItemName>qdMatch_Box</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function qdMatch_Box(tokenizer As QuickDrawTokenizer, ByRef model As X3Core.X3Model) As Boolean</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee} initiative.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim result As Boolean</SourceLine>
   <SourceLine>Dim orientationX As Double</SourceLine>
   <SourceLine>Dim orientationY As Double</SourceLine>
   <SourceLine>Dim orientationZ As Double</SourceLine>
   <SourceLine>Dim majorX As Double</SourceLine>
   <SourceLine>Dim majorY As Double</SourceLine>
   <SourceLine>Dim majorZ As Double</SourceLine>
   <SourceLine>Dim minorX As Double</SourceLine>
   <SourceLine>Dim minorY As Double</SourceLine>
   <SourceLine>Dim minorZ As Double</SourceLine>
   <SourceLine>Dim originX As Double</SourceLine>
   <SourceLine>Dim originY As Double</SourceLine>
   <SourceLine>Dim originZ As Double</SourceLine>
   <SourceLine>Dim vec as X3Core.X3Vector</SourceLine>
   <SourceLine>Dim poly As X3Core.X3Polygon</SourceLine>
   <SourceLine>Dim currentVertexIndex As Double</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = false</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.NextToken() = "Box" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match open bracket</SourceLine>
   <SourceLine>if tokenizer.NextToken() = "(" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.LookToken() &lt;&gt; ")" then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>orientationX = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>orientationY = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>orientationZ = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>majorX = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>majorY = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>majorZ = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>minorX = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>minorY = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>minorZ = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>originX = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>originY = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine>originZ = Val(tokenizer.NextToken())</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>currentVertexIndex = model.Vertex.Ubound</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' generate vertices of box</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>vec = new X3Core.X3Vector(0, 0, 0)</SourceLine>
   <SourceLine>model.Vertex.Append vec</SourceLine>
   <SourceLine>vec = new X3Core.X3Vector(majorX, majorY, majorZ)</SourceLine>
   <SourceLine>model.Vertex.Append vec</SourceLine>
   <SourceLine>vec = new X3Core.X3Vector(majorX + orientationX, majorY + orientationY, majorZ + orientationZ)</SourceLine>
   <SourceLine>model.Vertex.Append vec</SourceLine>
   <SourceLine>vec = new X3Core.X3Vector(orientationX, orientationY, orientationZ)</SourceLine>
   <SourceLine>model.Vertex.Append vec</SourceLine>
   <SourceLine>vec = new X3Core.X3Vector(minorX, minorY, minorZ)</SourceLine>
   <SourceLine>model.Vertex.Append vec</SourceLine>
   <SourceLine>vec = new X3Core.X3Vector(minorX + majorX, minorY + majorY, minorZ + majorZ)</SourceLine>
   <SourceLine>model.Vertex.Append vec</SourceLine>
   <SourceLine>vec = new X3Core.X3Vector(minorX + majorX + orientationX, minorY + majorY + orientationY, minorZ + majorZ + orientationZ)</SourceLine>
   <SourceLine>model.Vertex.Append vec</SourceLine>
   <SourceLine>vec = new X3Core.X3Vector(minorX + orientationX, minorY + orientationY, minorZ + orientationZ)</SourceLine>
   <SourceLine>model.Vertex.Append vec</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' generate polygons of box</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' face 0</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly = new X3Core.X3Polygon()</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 1</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 4</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 8</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 5</SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' face 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly = new X3Core.X3Polygon()</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 2</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 6</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 7</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 3</SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' face 2</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly = new X3Core.X3Polygon()</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 1</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 2</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 3</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 4</SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' face 3</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly = new X3Core.X3Polygon()</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 5</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 8</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 7</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 6</SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' face 4</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly = new X3Core.X3Polygon()</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 4</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 3</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 7</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 8</SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' face 5</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly = new X3Core.X3Polygon()</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 1</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 5</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 6</SourceLine>
   <SourceLine>poly.VIndex.Append currentVertexIndex + 2</SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' match closing bracket</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if tokenizer.NextToken() = ")" then</SourceLine>
   <SourceLine>result = true</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return result</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
